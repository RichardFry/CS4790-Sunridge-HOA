                                        <div class="col-5">
                                            <h6>--Mailing Address--</h6>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Street Address</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>8146 Deer Run Way</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>City</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>South Weber</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>State</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>Utah</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Zip Code</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>84405</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>--Contact--</h6>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Primary Phone</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>801-555-1234</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Email</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>rich@richfry.com</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>--In Case of Emergency--</h6>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Name</h6>
                                        </div>
                                        <div class="col-7">
                                            <p></p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Phone</h6>
                                        </div>
                                        <div class="col-7">
                                            <p></p>
                                        </div>
                                    </div>

                                </div>

                            </div>
                        </div>


                        <div class="col-6">
