                                <div class="col-12">
                                    <div class="row">
                                        <h5 id="subtitle">Information </h5>
                                        <a class="fas fa-edit float-right" id="edit" href="#"></a>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>First Name</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>Richard</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Last Name</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>Fry</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Birthday</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>07/07/1999</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>Occupation</h6>
                                        </div>
                                        <div class="col-7">
                                            <p>Professor</p>
                                        </div>
                                    </div>

                                    <div class="form-group row" id="formGroupLine">
                                        <div class="col-5">
                                            <h6>CoOwner</h6>
                                        </div>
